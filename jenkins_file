pipeline {
    agent any 
        stages {
            stage('Cloning repo') {
                steps {
                    echo 'Cloning the repo'
                }
            }
            stage('Build') {
                steps {
                    echo 'Building the project...'
                }
            }
            stage('Test'){
                steps {
                    echo 'Running tests...'
                }
            }

            
        }
}